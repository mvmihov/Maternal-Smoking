 "cells": [
  {
   "cell_type": "markdown",
   "id": "centered-wholesale",
   "metadata": {},
   "source": [
    "# Computing Blood Markers - MS correlations\n",
    "### Author: Mihail Mihov"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "spread-swift",
   "metadata": {},
   "outputs": [],
   "source": [
    "#load the necessary libraries for the project\n",
    "library(data.table)\n",
    "suppressMessages(library(tidyverse))\n",
    "library(broom)\n",
    "\n",
    "#expand the number of columns printed by jupyteR to see the whole table\n",
    "options(repr.matrix.max.cols = 1000)\n",
    "\n",
    "#load in the data table from UKBiobank\n",
    "pheno <- fread(file=\"Path_to_UKBiobank_phenotypes_file/file.txt\", data.table=F)\n",
    "\n",
    "#Fix the required columns to be in numeric format\n",
    "pheno\\$maternal_smoking=as.numeric(pheno\\$maternal_smoking)\n",
    "pheno\\$sex=as.numeric(pheno\\$sex)\n",
    "pheno\\$x_agebase=as.numeric(pheno\\$x_agebase)\n",
    "pheno\\$education=as.numeric(pheno\\$education)\n",
    "pheno\\$alcohol=as.numeric(pheno\\$alcohol)\n",
    "pheno\\$lack_of_PA=as.numeric(pheno\\$lack_of_PA)\n",
    "pheno\\$deprv_index=as.numeric(pheno\\$deprv_index)\n",
    "pheno\\$packyears=as.numeric(pheno\\$packyears)\n",
    "pheno\\$smoking_status=as.numeric(pheno\\$smoking_status, levels=c(0,1,2))\n",
    "pheno\\$x_WBC=as.numeric(pheno\\$x_WBC)\n",
    "phen\\\$x_RBC=as.numeric(pheno\\$x_RBC)\n",
    "pheno\\$x_lymphocytes=as.numeric(pheno\\$x_lymphocytes)\n",
    "pheno\\$x_platelet_count=as.numeric(pheno\\$x_platelet_count)\n",
    "pheno\\$x_neutrophill=as.numeric(pheno\\$x_neutrophill)\n",
    "pheno\\$x_Microalbumin=as.numeric(pheno\\$x_Microalbumin)\n",
    "pheno\\$x_Creatinine=as.numeric(pheno\\$x_Creatinine)\n",
    "pheno\\$x_APOA=as.numeric(pheno\\$x_APOA)\n",
    "pheno\\$x_APOB=as.numeric(pheno\\$x_APOB)\n",
    "pheno\\$x_Cholesterol=as.numeric(pheno\\$x_Cholesterol)\n",
    "pheno\\$x_HDL=as.numeric(pheno\\$x_HDL)\n",
    "pheno\\$x_LDL=as.numeric(pheno\\$x_LDL)\n",
    "pheno\\$x_Triglycerides=as.numeric(pheno\\$x_Triglycerides)\n",
    "pheno\\$x_CRP=as.numeric(pheno\\$x_CRP)\n",
    "pheno\\$x_Cystatin=as.numeric(pheno\\$x_Cystatin)\n",
    "pheno\\$x_HbA1c=as.numeric(pheno\\$x_HbA1c)\n",
    "pheno\\$x_IGF=as.numeric(pheno\\$x_IGF)\n",
    "pheno\\$x_VITD=as.numeric(pheno\\$x_VITD)\n",
    "pheno\\$x_GGT=as.numeric(pheno\\$x_GGT)\n",
    "pheno\\$x_ASP_AT=as.numeric(pheno\\$x_ASP_AT)\n",
    "pheno\\$x_ALA_AT=as.numeric(pheno\\$x_ALA_AT)\n",
    "pheno\\$x_AP=as.numeric(pheno\\$x_AP)\n",
    "\n",
    "#remove all records with sex chromosome abnormalities, non-European ancestry and missing maternal smoking record\n",
    "pheno=pheno[is.na(pheno\\$sex_aneu) & is.na(pheno\\$gen_exclude) & !is.na(pheno\\$gen_ethnicity) & (pheno\\$maternal_smoking!=\"missing\") & !is.na(pheno\\$maternal_smoking),]\n",
    "\n",
    "#change all NA values related to personal smoking status to \"missing\". This will make sure they are included during the calcualtion of the linear models.\n",
    "pheno[is.na(pheno\\$smoking_status), \"smoking_status\"]=\"missing\"\n",
    "\n",
    "\n",
    "#impute missing values for deprivation index and pack years to the median for the population\n",
    "pheno[is.na(pheno\\$deprv_index), \"deprv_index\"]=median(pheno\\$deprv_index, na.rm=T)\n",
    "pheno[is.na(pheno\\$packyears), \"packyears\"]=median(pheno\\$packyears, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_WBC), \"x_WBC\"]=median(pheno\\$x_WBC, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_RBC), \"x_RBC\"]=median(pheno\\$x_RBC, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_lymphocytes), \"x_lymphocytes\"]=median(pheno\\$x_lymphocytes, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_platelet_count), \"x_platelet_count\"]=median(pheno\\$x_platelet_count, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_neutrophill), \"x_neutrophill\"]=median(pheno\\$x_neutrophill, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_Microalbumin), \"x_Microalbumin\"]=median(pheno\\$x_Microalbumin, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_Creatinine), \"x_Creatinine\"]=median(pheno\\$x_Creatinine, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_APOA), \"x_APOA\"]=median(pheno\\$x_APOA, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_APOB), \"x_APOB\"]=median(pheno\\$x_APOB, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_Cholesterol), \"x_Cholesterol\"]=median(pheno\\$x_Cholesterol, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_HDL), \"x_HDL\"]=median(pheno\\$x_HDL, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_LDL), \"x_LDL\"]=median(pheno\\$x_LDL, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_Triglycerides), \"x_Triglycerides\"]=median(pheno\\$x_Triglycerides, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_CRP), \"x_CRP\"]=median(pheno\\$x_CRP, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_Cystatin), \"x_Cystatin\"]=median(pheno\\$x_Cystatin, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_HbA1c), \"x_HbA1c\"]=median(pheno\\$x_HbA1c, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_IGF), \"x_IGF\"]=median(pheno\\$x_IGF, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_VITD), \"x_VITD\"]=median(pheno\\$x_VITD, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_GGT), \"x_GGT\"]=median(pheno\\$x_GGT, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_ASP_AT), \"x_ASP_AT\"]=median(pheno\\$x_ASP_AT, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_ALA_AT), \"x_ALA_AT\"]=median(pheno\\$x_ALA_AT, na.rm=T)\n",
    "pheno[is.na(pheno\\$x_AP), \"x_AP\"]=median(pheno\\$x_AP, na.rm=T)\n",
    "\n",
    "#Fix all people that indicated non-smoker status to have pack years equal to 0\n",
    "pheno[(pheno\\$smoking_status)==0, \"packyears\"]=0\n",
    "\n",
    "#scale all continuous variables to have a mean = 0 and SD = 1\n",
    "pheno\\$x_BMI=scale(pheno\\$x_BMI)\n",
    "pheno\\$deprv_index=scale(pheno\\$deprv_index)\n",
    "pheno\\$packyears=scale(pheno\\$packyears)\n",
    "pheno\\$x_WBC=scale(pheno\\$x_WBC)\n",
    "pheno\\$x_RBC=scale(pheno\\$x_RBC)\n",
    "pheno\\$x_lymphocytes=scale(pheno\\$x_lymphocytes)\n",
    "pheno\\$x_platelet_count=scale(pheno\\$x_platelet_count)\n",
    "pheno\\$x_neutrophill=scale(pheno\\$x_neutrophill)\n",
    "pheno\\$x_Microalbumin=scale(pheno\\$x_Microalbumin)\n",
    "pheno\\$x_Creatinine=scale(pheno\\$x_Creatinine)\n",
    "pheno\\$x_APOA=scale(pheno\\$x_APOA)\n",
    "pheno\\$x_APOB=scale(pheno\\$x_APOB)\n",
    "pheno\\$x_Cholesterol=scale(pheno\\$x_Cholesterol)\n",
    "pheno\\$x_HDL=scale(pheno\\$x_HDL)\n",
    "pheno\\$x_LDL=scale(pheno\\$x_LDL)\n",
    "pheno\\$x_Triglycerides=scale(pheno\\$x_Triglycerides)\n",
    "pheno\\$x_CRP=scale(pheno\\$x_CRP)\n",
    "pheno\\$x_Cystatin=scale(pheno\\$x_Cystatin)\n",
    "pheno\\$x_HbA1c=scale(pheno\\$x_HbA1c)\n",
    "pheno\\$x_IGF=scale(pheno\\$x_IGF)\n",
    "pheno\\$x_VITD=scale(pheno\\$x_VITD)\n",
    "pheno\\$x_GGT=scale(pheno\\$x_GGT)\n",
    "pheno\\$x_ASP_AT=scale(pheno\\$x_ASP_AT)\n",
    "pheno\\$x_ALA_AT=scale(pheno\\$x_ALA_AT)\n",
    "pheno\\$x_AP=scale(pheno\\$x_AP)\n",
    "\n",
    "#Restric the database to the required parameters\n",
    "pheno=pheno[,c(\"studiepersonid\",\"maternal_smoking\",\"sex\",\"x_agebase\",\"x_BMI\", \"sex_aneu\", \"gen_exclude\", \"gen_ethnicity\",\n",
    "                        \"smoking_status\",\"education\",\"alcohol\",\"packyears\",\"lack_of_PA\",\"deprv_index\", \"x_WBC\", \"x_RBC\", \"x_lymphocytes\", \n",
    "                        \"x_platelet_count\", \"x_neutrophill\", \"x_Microalbumin\", \"x_Creatinine\", \"x_APOA\", \"x_APOB\", \"x_Cholesterol\",\n",
    "                        \"x_HDL\", \"x_LDL\", \"x_Triglycerides\", \"x_CRP\", \"x_Cystatin\", \"x_HbA1c\", \"x_IGF\", \"x_VITD\", \"x_GGT\", \"x_ASP_AT\", \"x_ALA_AT\", \"x_AP\")]\n",
    "head(pheno)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "diagnostic-oxygen",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Create female, male, smoker and non-smoker subgroups of the data \n",
    "pheno.f=pheno[pheno$sex==0,]\n",
    "pheno.m=pheno[pheno$sex==1,]\n",
    "pheno.smokers=pheno[pheno$smoking_status>0,]\n",
    "pheno.nonsmokers=pheno[pheno$smoking_status==0,]"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "structural-rebel",
   "metadata": {},
   "source": [
    "### Fit regression models between the choses variables and maternal smoking while accounting for confounders "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "necessary-consumer",
   "metadata": {},
   "outputs": [],
   "source": [
    "#create the first row of the dataframe (that will contain the beta values for all correlations) to be used for plotting later on\n",
    "bm.beta=data.frame(1,1)\n",
    "colnames(bm.beta)=c(\"term\",\"estimate\")\n",
    "\n",
    "#Fill the first column of the dataframe by fitting a linear model looping over all chosen blood markers\n",
    "#tidy(...) extracts the statistics of the model (e.g. fit.a) in table format\n",
    "for(i in 15:ncol(pheno))\n",
    "    {\n",
    "    fit.a=lm(pheno[,i]~maternal_smoking+sex+x_agebase+x_BMI+smoking_status+education+alcohol+lack_of_PA+deprv_index, data=pheno)\n",
    "    bm.beta=rbind(bm.beta, tidy(fit.a)[2,c(1,2)])\n",
    "}\n",
    "\n",
    "#Change the term column to indicate the correlated variable in the correct order\n",
    "bm.beta$term[-1]=colnames(pheno)[15:ncol(pheno)]\n",
    "\n",
    "#Name each column in the table accordingly (e.g. The first column is the overall or \"All\" analysis)\n",
    "colnames(bm.beta)[2]=\"All\"\n",
    "head(bm.beta)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "champion-february",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Create temporary dataframe to host the beta values from each subgroup\n",
    "bm.beta.temp=data.frame(1,1)\n",
    "colnames(bm.beta.temp)=c(\"term\",\"estimate\")\n",
    "\n",
    "#Repeat the linear regression analysis looping over each blood marker within each subgroup\n",
    "for(i in 1:4)\n",
    "    {\n",
    "    \n",
    "    if(i==1)\n",
    "    {\n",
    "        for(k in 15:ncol(pheno.f))\n",
    "        {\n",
    "            fit=lm(pheno.f[,k]~maternal_smoking+x_agebase+x_BMI+packyears+education+alcohol+lack_of_PA+deprv_index, data=pheno.f)\n",
    "            bm.beta.temp=rbind(bm.beta.temp, tidy(fit)[2,c(1,2)])\n",
    "        \n",
    "        }\n",
    "        colnames(bm.beta.temp)[2]=\"Female\"\n",
    "    }\n",
    "    \n",
    "    if(i==2)\n",
    "    {\n",
    "        for(k in 15:ncol(pheno.m))\n",
    "        {\n",
    "            fit=lm(pheno.m[,k]~maternal_smoking+x_agebase+x_BMI+packyears+education+alcohol+lack_of_PA+deprv_index, data=pheno.m)\n",
    "            bm.beta.temp=rbind(bm.beta.temp, tidy(fit)[2,c(1,2)])\n",
    "        }\n",
    "        colnames(bm.beta.temp)[2]=\"Male\"    \n",
    "    }\n",
    "\n",
    "    if(i==3)\n",
    "    {\n",
    "        for(k in 15:ncol(pheno.smokers))\n",
    "        {\n",
    "            fit=lm(pheno.smokers[,k]~maternal_smoking+sex+x_agebase+x_BMI+education+alcohol+lack_of_PA+deprv_index, data=pheno.smokers)\n",
    "            bm.beta.temp=rbind(bm.beta.temp, tidy(fit)[2,c(1,2)])\n",
    "        }\n",
    "    colnames(bm.beta.temp)[2]=\"Ever smokers\"\n",
    "    }\n",
    "\n",
    "    if(i==4)\n",
    "    {\n",
    "        for(k in 15:ncol(pheno.nonsmokers))\n",
    "        {\n",
    "            fit=lm(pheno.nonsmokers[,k]~maternal_smoking+sex+x_agebase+x_BMI+education+alcohol+lack_of_PA+deprv_index, data=pheno.nonsmokers)\n",
    "            bm.beta.temp=rbind(bm.beta.temp, tidy(fit)[2,c(1,2)])\n",
    "        }\n",
    "    colnames(bm.beta.temp)[2]=\"Never smokers\"\n",
    "    }\n",
    "    \n",
    "    bm.beta.temp$term[-1]=colnames(pheno)[15:ncol(pheno)]\n",
    "    \n",
    "    #bind the new columns to the main (bm.beta) dataframe\n",
    "    bm.beta=full_join(bm.beta,bm.beta.temp, by=\"term\")\n",
    "    \n",
    "    #Refresh the temporary dataframe\n",
    "    rm(bm.beta.temp)\n",
    "    bm.beta.temp=data.frame(1,1)\n",
    "    colnames(bm.beta.temp)=c(\"term\",\"estimate\")  \n",
    "}\n",
    "\n",
    "head(bm.beta)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "opening-prison",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Repeat the above steps to create a dataframe containing P-values (bm.p)\n",
    "#For that purpose change all tidy(fit.a)[2,c(1,2)] to tidy(fit.a)[2,c(1,5)]"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "coordinated-kentucky",
   "metadata": {},
   "source": [
    "### Plotting "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "renewable-procedure",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Remove the artificially created first row and the term column\n",
    "bm.beta=bm.beta[-1,]\n",
    "bm.beta=bm.beta[,-1]\n",
    "bm.p=bm.p[-1,]\n",
    "bm.p=bm.p[,-1]\n",
    "\n",
    "#Fix the rownames in preparation for plotting\n",
    "rownames(bm.beta)=c(\"White blood cells\", \"Red blood cells\", \"Lymphocytes\", \"Platelet count\", \"Neutrophill\", \n",
    "                   \"Microalbumin\", \"Creatinine\", \"APOA\", \"APOB\", \"Cholesterol\", \"HDL\", \"LDL\", \"Triglycerides\",\n",
    "                   \"CRP\", \"Cystatin\", \"HbA1c\", \"IGF\", \"VitD\", \"GGT\", \"AST\", \"ALT\", \"ALP\")\n",
    "\n",
    "head(bm.beta)\n",
    "\n",
    "rownames(bm.p)=c(\"White blood cells\", \"Red blood cells\", \"Lymphocytes\", \"Platelet count\", \"Neutrophill\", \n",
    "                   \"Microalbumin\", \"Creatinine\", \"APOA\", \"APOB\", \"Cholesterol\", \"HDL\", \"LDL\", \"Triglycerides\",\n",
    "                   \"CRP\", \"Cystatin\", \"HbA1c\", \"IGF\", \"VitD\", \"GGT\", \"AST\", \"ALT\", \"ALP\")\n",
    "\n",
    "head(bm.p)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "invalid-details",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Plot using package 'Corrplot'\n",
    "\n",
    "library(corrplot)\n",
    "#RColorBrewer expands the choice of color legends\n",
    "library(RColorBrewer)\n",
    "options(repr.plot.width=10, repr.plot.height=6, repr.plot.res=240)\n",
    "par(bg = 'white')\n",
    "\n",
    "options(repr.plot.width=9, repr.plot.height=3, repr.plot.res=500)\n",
    "corrplot(t(bm.beta),na.label = \"square\", na.label.col = rgb(200,200, 200, max = 255, alpha = 255),is.corr = FALSE, method =c(\"circle\"),\n",
    "         addgrid.col = \"grey\", col = brewer.pal(10, \"RdBu\")[10:1], \n",
    "         tl.cex = 1.5, tl.col = \"black\", tl.srt=45,p.mat=t(bm.p), \n",
    "         sig.level = c(.05), mar = c(1, 1, 0,1),\n",
    "         pch.cex = 2.2, cl.cex=1.5, cl.align.text=\"l\", cl.ratio = .1, outline=TRUE, , pch.col = \"black\", pch=\"*\",insig = \"label_sig\")\n",
    "\n",
    "#Export the figure to PDF\n",
    "dev.copy2pdf(width=20, height=10, file = \"~/Your_Directory/Corrplot_Blood_Markers.pdf\", out.type=\"pdf\")\n",
    "dev.off()"
   ]
  }
